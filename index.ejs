<html lang="pt-BR">
  <head>
    <meta charset="UTF-8" />
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <title><%= site_config.titulo_pagina%></title>
    <meta content="Equipe" name="description" />
    <link href="https://cdn.jsdelivr.net/gh/hung1001/font-awesome-pro@4cac1a6/css/all.css" rel="stylesheet" type="text/css"/>
    <link href="<%= site_config.particulas%>" rel="icon" />
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet"/>
    <link href="https://unpkg.com/aos@2.3.1/dist/aos.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2"crossorigin="anonymous"/>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/glightbox@3.2.0/dist/css/glightbox.min.css"/>
    <link href="style.css" rel="stylesheet" />
    <meta property="og:type" content="website" />
    <meta name="theme-color" content="#301934" />
    <meta property="og:description" content="The best family of Discord." />
    <meta property="og:title" content="1533 Family" />
    <meta property="og:image" content="https://cdn.discordapp.com/attachments/797190863007645726/1126661749047836753/sim.jpg"/>
  </head>

  <body>
    <div id="clickMessage" class="server-div">
      <div class="title-animation" id="revealButton" onclick="revealSite()">
        <span style="--i: 1">1</span>
        <span style="--i: 2">5</span>
        <span style="--i: 3">3</span>
        <span style="--i: 4">3</span>
      </div>
    </div>

    <div id="conteudo_site" style="display: none">
      <% if(server !== null) { %>
      

        <div class="section-title">
          <h2 style="margin-bottom: 10px; margin-top: 13px;" data-aos="fade-in">
            TEAM MEMBERS
          </h2>
        </div>
  
      <section id="team" class="team section-bg">
        <% } else { %>
        <div class="section-title">
          <h2 data-aos="fade-in">TEAM MEMBERS</h2>
        </div>
        <% } %>
        <div class="row justify-content-md-center" style="margin-top: 20px; margin-left: 5px">
          <% Object.values(users).forEach((a) => { %>

          <div class="col-xl-3 col-lg-4 col-md-6">
            <div class="member">
              <div class="pic">
                <img src="<%=a.info_user.avatar%>";/>
              </div>
              <h1 style="color: rgb(255, 250, 250)"><%=a.info_user.global_name%></h1>
              <ul class="badge-list">
                <% a.badges.forEach((badge) => { %>
                <li>
                  <img class="badge-image" src="<%= badge %>" alt="Badge" />
                </li>
               
                <% }); %>
                <li>
              
                </li>
              </ul>

            </div>
          </div>
          
          <% }) %>
        </div>
        <div class="friends-title">
          <h2 style="margin-bottom: 10px; margin-top: 30px;" data-aos="fade-in">FRIENDS</h2>
        </div>
        <div class="row justify-content-md-center" style="margin-top: 20px; margin-left: 5px">
          <% Object.values(amigos).forEach((a) => { %>

          <div class="col-xl-2 col-lg-4 col-md-6">
            <div class="friend">
              <div class="pic">
                <img src="<%=a.friend_info.avatar%>";/>
              </div>
              <h1 style="color: #fff"><%=a.friend_info.global_name%></h1>
              <ul class="badge-list">
                <% a.badges.forEach((badge) => { %>
                <li>
                  <img class="badge-image" src="<%= badge %>" alt="Badge" />
                </li>
                
                <% }); %>
                <li>

                </li>
              </ul>

            </div>
          </div>

          <% }) %>
         

        </div>

      </section>

      <div data-aos="fade-in" class="server-div" style="padding-top: 135px; margin-bottom: 20px">
        <button class="button" id="salvesjs" onclick="salvesLoad()">Salves</button>
        <img style="border-radius: 50%; margin-bottom: 20px; margin-top: 20px;" src="<%= server.iconURL({dynamic: true, extension: 'webp'}) %>" alt="Imagem" class="img"/>
        <h2><%= server.name %></h2>
    <button class="button" onclick="window.open('<%= sv.convite%>')" style="height: 50px">Entrar</button>
      </div>
    </div> 


    <div id="conteudosalves" style="display: none">
      <% if(server !== null) { %>
        <div class="section-title">
          <h2 style="margin-bottom: 10px; padding-top: 13px;" data-aos="fade-in">
            Salves
          </h2>
        </div>
  
      <section id="team" class="team section-bg">
        <% } else { %>
        <div class="section-title">
        </div>
        <% } %>
        <div class="row justify-content-md-center" style="margin-top: 20px; margin-left: 5px">
          <% Object.values(salvers).forEach((a) => { %>
          <div class="col-xl-2 col-lg-4 col-md-6">
            <div class="salvadoslist">
              <h1><%=a.salvado_info.nome%></h1>
            </div>
            
          </div>
          <% }) %>
         
        </div>
      <center>
        <button class="button" onclick="back()">Back to home</button> 
      </center>
    <script>
      async function salvesLoad() {
        var clickMessage = document.getElementById("conteudo_site");
        var conteudo_site = document.getElementById("conteudosalves");

        clickMessage.style.display = "none";

        conteudo_site.classList.add("fade-in");

        setTimeout(function () {
          conteudo_site.style.display = "block";
        }, 100);
      }
    </script>

<script>
  async function back() {
    var clickMessage = document.getElementById("conteudosalves");
    var conteudo_site = document.getElementById("conteudo_site");

    clickMessage.style.display = "none";

    conteudo_site.classList.add("fade-in");

    setTimeout(function () {
      conteudo_site.style.display = "block";
    }, 100);
  }
</script>

    <script>
      async function revealSite() {
        var clickMessage = document.getElementById("clickMessage");
        var conteudo_site = document.getElementById("conteudo_site");
        var musicas = ["shit_trap", "priv8", "pitbull", "os_menino"];
        var random = Math.floor(Math.random() * musicas.length);
        var music = musicas[random] + ".mp3";
        var audio = new Audio(`./musicas/${music}`);
        audio.volume = 0.2;
        audio.loop = true;

        clickMessage.style.display = "none";
     
        conteudo_site.classList.add("fade-in") ;
          
        setTimeout(function () {
          conteudo_site.style.display = "block";
        }, 100);

        audio.play();
      }
    </script>

